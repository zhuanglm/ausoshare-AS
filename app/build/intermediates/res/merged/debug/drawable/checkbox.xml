<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true"
        android:drawable="@drawable/checkbox_marked_wh" /> <!-- checked -->
    <item android:state_checked="false"
        android:drawable="@drawable/checkbox_unmarked_wh" /> <!-- unchecked -->
    <item android:state_focused="true"
        android:drawable="@drawable/checkbox_unmarked_wh" />    <!--on focus-->
    <item android:drawable="@drawable/checkbox_unmarked_wh" /> <!-- default -->
</selector>